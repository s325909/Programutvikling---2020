<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ccomp.admin.AdminController">
    <children>
        <TabPane layoutY="100.0" prefHeight="500.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="SE PRODUKTER">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TabPane prefHeight="453.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE">
                           <tabs>
                              <Tab text="Motor">
                                 <content>
                                    <TableView prefHeight="200.0" prefWidth="200.0">
                                      <columns>
                                        <TableColumn prefWidth="138.0" text="Navn" />
                                          <TableColumn prefWidth="138.0" text="Hestekrefter" />
                                          <TableColumn prefWidth="138.0" text="Pris" />
                                          <TableColumn prefWidth="138.0" text="Antall" />
                                      </columns>
                                    </TableView>
                                 </content>
                              </Tab>
                              <Tab text="Sete">
                                 <content>
                                    <TableView fx:id="seatView" prefHeight="200.0" prefWidth="200.0">
                                      <columns>
                                        <TableColumn fx:id="nameSeatColum"  prefWidth="150.0" text="Navn" />
                                          <TableColumn fx:id="materiellColum"  prefWidth="150.0" text="Materiale" />
                                          <TableColumn fx:id="colorSeatColum"  prefWidth="150.0" text="Farge" />
                                          <TableColumn fx:id="seatPriceColum"  prefWidth="150.0" text="Pris" />
                                          <TableColumn fx:id="quantitySeatColum"  prefWidth="150.0" text="Antall" />
                                      </columns>
                                    </TableView>
                                 </content>
                              </Tab>
                              <Tab text="Spoiler">
                                 <content>
                                    <TableView fx:id="spoilerView" prefHeight="200.0" prefWidth="200.0">
                                      <columns>
                                        <TableColumn fx:id="nameSpoilerColum" prefWidth="146.0" text="Navn" />
                                          <TableColumn fx:id="colorSpoilerColum" prefWidth="146.0" text="Farge" />
                                          <TableColumn fx:id="sideSpoilerColum" prefWidth="146.0" text="Side" />
                                          <TableColumn fx:id="priceSpoilerColum" prefWidth="146.0" text="Pris" />
                                          <TableColumn fx:id="quantitySpoilerColum" prefWidth="146.0" text="Antall" />
                                      </columns>
                                    </TableView>
                                 </content>
                              </Tab>
                              <Tab text="Ratt">
                                 <content>
                                    <TableView fx:id="sWheelView" prefHeight="200.0" prefWidth="200.0">
                                      <columns>
                                        <TableColumn fx:id="nameSWheelColum" prefWidth="150.0" text="Navn" />
                                          <TableColumn fx:id="materiellSWeel" prefWidth="150.0" text="Materiale" />
                                          <TableColumn fx:id="colorSWheelColum" prefWidth="150.0" text="Farge" />
                                          <TableColumn fx:id="priceSWeelColum" prefWidth="150.0" text="Pris" />
                                          <TableColumn fx:id="quantitySWeelColum" prefWidth="150.0" text="Antall" />
                                      </columns>
                                    </TableView>
                                 </content>
                              </Tab>
                              <Tab text="Felg">
                                 <content>
                                    <TableView prefHeight="200.0" prefWidth="200.0">
                                      <columns>
                                        <TableColumn prefWidth="150.0" text="Navn" />
                                          <TableColumn prefWidth="150.0" text="Dimensjon" />
                                          <TableColumn prefWidth="150.0" text="Farge" />
                                          <TableColumn prefWidth="150.0" text="Pris" />
                                          <TableColumn prefWidth="150.0" text="Antall" />
                                      </columns>
                                    </TableView>
                                 </content>
                              </Tab>
                           </tabs>
                        </TabPane>
                     </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="BESTILLINGER">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <TableView layoutY="-2.0" prefHeight="460.0" prefWidth="800.0">
                                    <columns>
                                        <TableColumn prefWidth="75.0" text="Type" />
                                        <TableColumn prefWidth="75.0" text="Navn" />
                                        <TableColumn prefWidth="75.0" text="Mobil" />
                                        <TableColumn prefWidth="142.0" text="Komponent" />
                                        <TableColumn minWidth="8.0" text="Pris" />
                                    </columns>
                                    <effect>
                                        <InnerShadow choke="0.8" color="#eaae3d" />
                                    </effect>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
            <effect>
                <InnerShadow choke="1.0" color="#41a2a666" />
            </effect>
        </TabPane>
        <ImageView fitHeight="100.0" fitWidth="223.0" layoutX="289.0">
            <image>
                <Image url="@../../../resources/CCOMPW-small.png" />
            </image>
        </ImageView>
        <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="40.0" prefWidth="67.0" text="BACK">
         <font>
            <Font name="Segoe UI Semilight" size="18.0" />
         </font></Button>
      <Button fx:id="addComp" layoutX="680.0" layoutY="15.0" mnemonicParsing="false" onAction="#toAddComponent" prefHeight="39.0" prefWidth="106.0" text="LEGG TIL">
         <font>
            <Font name="Segoe UI Semilight" size="18.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
